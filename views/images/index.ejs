<% layout(layoutPath) -%>
<ul>
<% for(var i=0; i<images.length; i++) {%>
   <li><%= images[i] %></li>
<% } %>
</ul>
<form action="/upload" method="post" enctype="multipart/form-data">
  <div>
      <label>Image:</label>
      <input type="file" name="file"/>
  </div>
  <div>
    <button class="btn">Upload</button>
  </div>
</form>